<Configuration status="WARN">
  <Appenders>
    <File name="FileLogger" fileName="logs/test.log">
      <PatternLayout pattern="%d [%t] %-5level: %msg%n%throwable"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="FileLogger"/>
    </Root>
  </Loggers>
</Configuration>
